    "@vitejs/plugin-react": "^4.3.3",
    "@wyw-in-js/vite": "^0.5.4",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-plugin-macros": "^3.1.0",
